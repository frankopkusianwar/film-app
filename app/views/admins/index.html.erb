<table class="table table-striped table-dark shadow mt-5 ">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Contact</th>
      <th scope="col">Email</th>
      <th scope="col">Approved</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @user.each do |user| %>
      <p><%= user.name %></p>
      <tr>
          <th scope="row">1</th>
          <td><%= user.name %></td>
          <td><%= user.phone %></td>
          <td><%= user.email %></td>
          <td><%= user.approved %></td>
          <td><button class="btn-primary">Delete</button></td>
        </tr>
    <% end %>
  </tbody>
</table>
